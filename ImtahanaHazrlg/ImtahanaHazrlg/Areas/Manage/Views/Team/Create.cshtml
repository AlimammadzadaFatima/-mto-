@model Team
@{
    ViewData["Title"] = "Create";
}

<form method="post" asp-action="Create" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="FullName">FullName</label>
        <input asp-for="FullName" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="FullName">

    </div>
    <div class="form-group">
        <label asp-for="Profession">Profession</label>
        <input asp-for="Profession" class="form-control" id="exampleInputPassword1" placeholder="Profession">
    </div>
    <div class="form-group">
        <label asp-for="FormFile">ImageFile</label>
        <input asp-for="FormFile" class="form-control">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>
@section Scripts {
@Html.Partial("_ValidationScriptsPartial")
}
