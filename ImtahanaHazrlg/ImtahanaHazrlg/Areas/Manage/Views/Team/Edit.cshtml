@model Team
@{
    ViewData["Title"] = "Edit";
}

<form method="post" asp-action="Edit">
    <div class="form-group">
        <label asp-for="FullName">FullName</label>
        <input asp-for="FullName" class="form-control" placeholder="FullName">
    </div>
    <div class="form-group">
        <label asp-for="Profession">Profession</label>
        <input asp-for="Profession" class="form-control" placeholder="Profession">
    </div>
    <div class="form-group">
        <label asp-for="FormFile">Profession</label>
        <input asp-for="FormFile" class="form-control" placeholder="Profession">
        <img width="100" src="~/uploads/teams/@Model.Image" />
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>


